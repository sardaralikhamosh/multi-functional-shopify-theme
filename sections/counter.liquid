{%- capture section_style -%}
    {%- if section.settings.color_bg != 'rgba(0,0,0,0)' -%}
        background-color: {{ section.settings.color_bg }};
    {%- endif -%}
    {%- if section.settings.padding_block != blank -%}
        padding: {{section.settings.padding_block}};
    {%- endif -%}
    {%- if section.settings.margin_block != blank -%}
        margin: {{section.settings.margin_block}};
    {%- endif -%}
    {%- if section.settings.image_bg != blank -%}
        background: url({{section.settings.image_bg | img_url: 'master' }}) center center no-repeat;
        background-size: cover;
    {%- endif -%}
{%- endcapture -%}
<div class="vela-section counter overflow-hidden" {% if section_style%}style="{{ section_style }}"{% endif%}>
    <div class="container{% if section.settings.full_with %}-fluid{% endif %}">
        <div class="vela-section__inner{% if section.settings.color_bg != 'rgba(0,0,0,0)' %} py-5{% endif %}">
            <div class="vela-section__content">
                <div class="row g-4 ">
                    {% for block in section.blocks %}
                        <div class="counter__box col-12 col-md-6 col-lg-3">
                            <div class="counter__box-inner text-center">
                                <div class="counter__icon mb-3 pb-2 text-white">
                                    {%- if block.settings.featureicon != blank -%}
                                        <img class="img-fluid" src="{{ block.settings.featureicon | img_url: '60x' }}" alt="" />
